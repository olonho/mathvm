int x;
int y;
int z;

function int foo(int x) {
    int r;
    function int bar(int a) {
        if (a <= 0) {
            return z;
        }
        x = baz();
        r += 1;
        return x + bar(a - 1);
    }
    function int baz() {
        int s;
        function int bak(int z) {
            if (z <= 0) {
                x = z;
            } else {
                y = z + 1;
                x = -z;
            }
            x -= 1;
            int t;
            t = baz() - 1;
            if (s < 0) {
                s = s * -1;
            }
            x += 1;
            return t + z + bar(z - y);
        }
        if (x <= 0 || y <= 0 || z <= 0) {
            return x + y + z;
        }
        s = foo(x - 1);
        y += s;
        z = bak(z - y + 1) - 2;
        return s + bar(x);
    }
    if (x == 0) {
        return bar(baz() - 3);
    }
    r = foo(x - 1);
    bar(r);
    return r + baz();
}

x = 2;
y = 2;
z = 2;

print(foo(2), '\n');
print(x, '\n', y, '\n', z, '\n');
